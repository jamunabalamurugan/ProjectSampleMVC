@model Rash_Airlines.Models.Passenger_booking_details

@{
    ViewBag.Title = "Show Booked Ticket";
    Layout = "~/Views/Shared/_Logged_layout.cshtml";

    }


 
<html>
<head>
    <link href="~/Content/ShowBookedTicket.css" rel="stylesheet" />

    @*<script src="~/Content/Download.js"></script>*@
    <style>
        .f
        {
        margin-top:600px;
        }
      
    </style>
</head>

<body>


    <div class="box">
        <ul class="left">
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
        </ul>

        <ul class="right">
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
        </ul>
        <div class="ticket">
            <span class="airline">RASH AIRLINES</span>
            @*<span class="airline airlineslip">RASH AIRLINES</span>*@
            <span class="boarding">Boarding pass</span>
            <div class="content">
                <span class="jfk">@TempData["departure"]<br />  @Html.DisplayFor(model => model.Flights_Master.Flights_Schedule.First().departure_time) </span>
                <span class="plane">
                    <?xml version="1.0" ?>
                    <svg clip-rule="evenodd" fill-rule="evenodd" height="60" width="60" image-rendering="optimizeQuality" shape-rendering="geometricPrecision" text-rendering="geometricPrecision" viewBox="0 0 500 500" xmlns="http://www.w3.org/2000/svg"><g stroke="#222"><line fill="none" stroke-linecap="round" stroke-width="30" x1="300" x2="55" y1="390" y2="390" /><path d="M98 325c-9 10 10 16 25 6l311-156c24-17 35-25 42-50 2-15-46-11-78-7-15 1-34 10-42 16l-56 35 1-1-169-31c-14-3-24-5-37-1-10 5-18 10-27 18l122 72c4 3 5 7 1 9l-44 27-75-15c-10-2-18-4-28 0-8 4-14 9-20 15l74 63z" fill="#222" stroke-linejoin="round" stroke-width="10" /></g></svg>
                </span>
                <span class="sfo"> @TempData["Arrival"] <br>  @Html.DisplayFor(model => model.Flights_Master.Flights_Schedule.First().arrival_time)</span>

                <span class="jfk jfkslip">@TempData["departure"]<br> @Html.DisplayFor(model => model.Flights_Master.Flights_Schedule.First().departure_time)  </span>
                <span class="plane planeslip">
                    <?xml version="1.0" ?>
                    <svg clip-rule="evenodd" fill-rule="evenodd" height="50" width="50" image-rendering="optimizeQuality" shape-rendering="geometricPrecision" text-rendering="geometricPrecision" viewBox="0 0 500 500" xmlns="http://www.w3.org/2000/svg"><g stroke="#222"><line fill="none" stroke-linecap="round" stroke-width="30" x1="300" x2="55" y1="390" y2="390" /><path d="M98 325c-9 10 10 16 25 6l311-156c24-17 35-25 42-50 2-15-46-11-78-7-15 1-34 10-42 16l-56 35 1-1-169-31c-14-3-24-5-37-1-10 5-18 10-27 18l122 72c4 3 5 7 1 9l-44 27-75-15c-10-2-18-4-28 0-8 4-14 9-20 15l74 63z" fill="#222" stroke-linejoin="round" stroke-width="10" /></g></svg>
                </span>
                <span class="sfo sfoslip">
                    @TempData["Arrival"]<br>
                    @Html.DisplayFor(model => model.Flights_Master.Flights_Schedule.First().arrival_time)
                </span>
                <div class="sub-content">
                    @*<span class="watermark">Rash Airlines</span>*@
                    <span class="name">PASSENGER NAME<br><span>@TempData["Name"]</span></span>
                    <span class="flight">SEAT NO.<br><span>@TempData["seat_no"]</span></span>
                    <span class="gate">BOOKING ID<br><span>@Html.DisplayFor(model => model.booking_id)</span></span><br />
                    <span class="boardingtime">CLASS<br><span>@Html.DisplayFor(model => model.@class)</span></span>

                    <span class="seat">JOURNEY DATE<br><span>@Html.DisplayFor(model => model.journey_date)</span></span>

                    <span class="flight flightslip">BOOKING ID<br><span>@Html.DisplayFor(model => model.booking_id)</span></span>
                    <span class="seat seatslip">SEAT NO.<br><span>@TempData["seat_no"]</span></span>
                    <span class="name nameslip">PASSENGER NAME<br><span>@TempData["Name"]</span></span>
                </div>
            </div>
            <div class="barcode"></div>
            <div class="barcode slip"></div>
        </div>
    </div>
    <div class="f"></div>
         
   
   
</body>
</html>
