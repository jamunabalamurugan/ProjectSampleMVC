

@{
    ViewBag.Title = "Search Page";
}



<br><br>
<div class="container">
    <div class="navbar topnav navbar-fixed-top pull-left">

        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            @Html.ActionLink("RASH Airlines", "Search", "SearchFlight", new { area = "" }, new { @class = "navbar-brand" })
        </div>

        <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav pull-right">
                <li>@Html.ActionLink("About", "About", "Home")</li>
                <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                <li>@Html.ActionLink("Register", "Create_Passenger", "Passenger_Reg")</li>
                @*<li>@Html.ActionLink("Login", "Login_Passenger", "Passenger_Login")</li>*@




            </ul>

        </div>
    </div>
</div>





