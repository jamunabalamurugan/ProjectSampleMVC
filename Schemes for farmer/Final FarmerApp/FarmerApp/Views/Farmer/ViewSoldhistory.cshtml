@model IEnumerable<FarmerApp.Soldhistory>

@{
    ViewBag.Title = "ViewSoldhistory";
}

<h2>View Soldhistory</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Crop_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Quantity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Baseprice)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Soldprice)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Totalprice)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SellRequest.Crop_type)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Date)


            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Crop_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Quantity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Baseprice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Soldprice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Totalprice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SellRequest.Crop_type)
            </td>

        </tr>
    }

</table>

<td>
    @Html.ActionLink("Go Back", "Index")
</td>
